use_frameworks!
inhibit_all_warnings!



def layout
    pod 'SnapKit', '~> 4.0.0'
    pod 'pop', '~> 1.0'
    pod 'CardSlider'
    pod 'expanding-collection'
end

def lib
    #pod 'KeychainAccess'
    #pod 'WCDB.swift'
    #pod 'AudioPlayerSwift'
    pod 'PromiseKit'
    pod 'RxSwift'
    pod 'Kingfisher'
    pod 'SwiftyJSON'
    pod 'Moya'
end


target "FunnyFm" do
    layout
    lib
end
