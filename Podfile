source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '11.1'
use_frameworks!
inhibit_all_warnings!



def layout
    pod 'SnapKit', '~> 4.0.0'
    pod 'pop', '~> 1.0'
    pod 'SHFullscreenPopGestureSwift'
    pod 'SPStorkController'
    pod 'lottie-ios'
    pod 'CBFlashyTabBarController'
    pod 'CleanyModal', '~> 0.1.1'
end

def lib
    pod 'KeychainAccess'
    pod 'WCDB.swift'
    pod 'JPush'
    pod 'RxSwift'
    pod 'Kingfisher'
    pod 'SwiftyJSON'
    pod 'Moya'
    pod 'WechatOpenSDK'
    pod 'AppCenter'
end


target "FunnyFm" do
    layout
    lib
end
